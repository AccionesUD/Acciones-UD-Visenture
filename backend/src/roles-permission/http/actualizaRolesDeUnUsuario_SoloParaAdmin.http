#Endpoint para actualizar el rol de un usuario (solo para admins)
PUT http://localhost:3000/api/users/987654321/rol
Authorization: Bearer <token_de_usuario_admin>
Content-Type: application/json
Body:
{
  "roleIds": [2, 3]
}

Response:
{
	"message": "Rol actualizado",
	"roles": [
		"comisionista",
		"admin"
	]
}