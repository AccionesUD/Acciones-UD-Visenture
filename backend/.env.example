# Configuración del servidor
PORT=3000
NODE_ENV=development

# Configuración de la base de datos
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=visenture_db

# Configuración JWT
JWT_SECRET=your_jwt_secret_key_example
JWT_EXPIRES_IN=1d
JWT_REFRESH_SECRET=your_refresh_token_secret_key_example
JWT_REFRESH_EXPIRES_IN=7d

# Configuración de correo electrónico
MAIL_HOST=smtp.example.com
MAIL_PORT=587
MAIL_USER=user@example.com
MAIL_PASSWORD=mail_password_example
MAIL_FROM=noreply@visenture.com

# URLs del frontend para redirecciones
FRONTEND_URL=http://localhost:4200
