{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "node",
      "@angular/localize"
    ]
  },
    "angularCompilerOptions": {
    "strictTemplates": false
  }, /*Eliminar o comentar lineas 12 hasta 14 para manjear errror*/
  "files": [
    "src/main.ts",
    "src/main.server.ts",
    "src/server.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}